{
    "manifest_version": 3,
    "name": "Frankie",
    "description": "OpenAI based virtual assistant",
    "version": "1.0",
    "host_permissions": [
      "https://api.openai.com/v1/completions"
    ],
    "permissions": [
      "storage",
      "tabs",
      "contextMenus"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "options_page": "options/options.html"
  }